{
  "name": "demo",
  "version": "0.0.1",
  "devDependencies": {
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.3.6",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.0"
  },
  "private": true,
  "scripts": {
    "fix": "run-s fix:prettier",
