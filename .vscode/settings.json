{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "java.checkstyle.configuration": "/google_checks.xml",
  "java.checkstyle.properties": {
    "org.checkstyle.google.suppressionfilter.config": "${workspaceFolder}/config/checkstyle/suppressions.xml",
    "org.checkstyle.google.suppressionxpathfilter.config": "${workspaceFolder}/config/checkstyle/suppressions-xpath.xml"
  },
  "java.compile.nullAnalysis.mode": "automatic",
  "java.compile.nullAnalysis.nonnull": ["org.jspecify.annotations.NonNull"],
  "java.compile.nullAnalysis.nonnullbydefault": [
    "org.jspecify.annotations.NullMarked"
  ],
  "java.compile.nullAnalysis.nullable": ["org.jspecify.annotations.Nullable"],
  "java.completion.importOrder": ["#", ""],
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.format.settings.google.version": "latest",
  "[java]": {
    "editor.codeActionsOnSave": {
      "source.generate.finalModifiers": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "josevseb.google-java-format-for-vs-code"
  }
}
