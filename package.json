{
  "name": "demo",
  "version": "0.0.1",
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@secretlint/secretlint-rule-preset-recommend": "^8.2.3",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "eslint": "^8.57.0",
    "eslint-config-ponko2": "^22.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-testing-library": "^6.2.1",
    "eslint-plugin-vitest": "^0.4.1",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.2",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "rimraf": "^5.0.5",
    "secretlint": "^8.2.3",
    "typescript-eslint": "^7.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@10.2.4",
  "private": true,
  "scripts": {
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p --aggregate-output -c -l lint:*",
    "lint:eslint": "eslint . --ext js,jsx,cjs,mjs,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier . --check",
    "lint:secretlint": "secretlint \"**/*\"",
    "prepare": "is-ci || husky"
  },
  "type": "module",
  "workspaces": [
    "frontend"
  ]
}
